vatarForm({ setShowModal, auth }) {\n\n    const [loading, setLoading] = useState(false)\n\n    const [updateAvatar] = useMutation(UPDATE_AVATAR, {\n        update(cache, { data:{ updateAvatar} }){\n           const { getUser } = cache.readQuery({\n               query: GET_USER,\n               variables: { username: auth.username}\n           })\n\n           cache.writeQuery({\n                query: GET_USER,\n                variables: { username: auth.username},\n                data: {\n                    getUser: { \n                        ...getUser,\n                        avatar: updateAvatar.urlAvatar\n                    }\n                }\n           })\n        }\n    })\n\n    const [deleteAvatar] = useMutation(DELETE_AVATAR,{\n        update(cache){\n            const { getUser } = cache.readQuery({\n                query: GET_USER,\n                variables: {username: auth.username}\n            })\n\n            cache.writeQuery({\n                query: GET_USER,\n                variables: { username: auth.username },\n                data: { \n                    getUser: { \n                        ...getUser,\n                        avatar: \"\"\n                    }\n                }\n            })\n        }\n    })\n\n    const onDrop = useCallback( async (acceptedFile) => {\n        const file = acceptedFile[0];\n\n        try {\n            setLoading(true)\n            const { data } = await updateAvatar({ variables: { file } })\n            if (!data.updateAvatar.status) {\n                toast.warning(\"Error al actualizar el avatar\")\n                setLoading(false);\n            }else {\n                setLoading(false)\n                setShowModal(false)\n            }\n        } catch (error) {\n            console.log(error)\n        }\n    }, [])\n\n    const { getRootProps, getInputProps} = useDropzone({\n        accept: \"image/jpeg, image/png\",\n        noKeyboard: true,\n        multiple: false,\n        onDrop,\n    })\n\n    const onDeleteAvatar = async () => {\n        try {\n            const { data } = await deleteAvatar();\n            \n            if (!data.deleteAvatar) {\n                toast.warning(\"Error al borrar el avatar\")\n            }else {\n                setShowModal(false);\n            }\n\n        } catch (error) {\n            console.log(error)\n        }\n\n    }\n\n    return (\n        <div className=\"avatar-form\">\n            <Button  {...getRootProps()} loading={loading}>Cargar una Foto</Button>\n            <Button onClick={onDeleteAvatar}>Eliminar Foto Actual</Button>\n            <Button onClick={() =>setShowModal(false)}>Cancelar</Button>\n            <input {...getInputProps()}/>\n        </div>\n    )\n}\n","/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Header/RightHeader/RightHeader.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/UserNotFound/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Modal/ModalBasic/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/UserNotFound/UserNotFound.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Modal/ModalBasic/ModalBasic.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Profile/HeaderProfile/HeaderProfile.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/SettingsForm/SettingsForm.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/PasswordForm/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/PasswordForm/PasswordForm.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/EmailForm/EmailForm.js",[],{"ruleId":"160","replacedBy":"161"},{"ruleId":"162","replacedBy":"163"},{"ruleId":"164","severity":1,"message":"165","line":7,"column":11,"nodeType":"166","messageId":"167","endLine":7,"endColumn":15},{"ruleId":"168","severity":1,"message":"1