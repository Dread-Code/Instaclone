/dreadfulcrazy/Documentos/Instaclone/client/src/routes/Navigation.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/Auth/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/utils/constants.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/routes/routes.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/Auth/Auth.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/layouts/LayoutBasic.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/User.js",["216"],"import React, { useEffect, useState } from \"react\";\nimport { useParams } from \"react-router-dom\";\nimport { size } from \"lodash\";\nimport Profile from \"../components/User/Profile\";\nimport { useQuery, useSubscription, useApolloClient } from \"@apollo/client\";\nimport { GET_PUBLICATIONS, WS_GET_PUBLICATIONS } from \"../gql/publication\";\nimport Publications from \"../components/User/Publications/Publications\";\n\nexport default function User() {\n  const { username } = useParams();\n  const client = useApolloClient();\n\n  const [publications, setPublications] = useState(null);\n\n  const { data, loading } = useQuery(GET_PUBLICATIONS, {\n    variables: {\n      username,\n    },\n  });\n\n  const { data: dataWsPublications } = useSubscription(WS_GET_PUBLICATIONS, {\n    variables: { username },\n  });\n\n  useEffect(() => {\n    setPublications(data?.getPublications.publications);\n  }, [data]);\n\n  useEffect(() => {\n    if (dataWsPublications) {\n      setPublications(dataWsPublications?.newPublication.publications);\n      client.writeQuery({\n        query: GET_PUBLICATIONS,\n        data: {\n          getPublications: {\n            ...dataWsPublications.newPublication,\n          },\n        },\n        variables: {\n          username,\n        },\n      });\n    }\n  }, [dataWsPublications])\n\n  if (loading) return null;\n  return (\n    <>\n      <Profile username={username} totalPublications={size(publications)} />\n      <Publications publications={publications} />\n    </>\n  );\n}\n","/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/Home/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/Error404/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Auth/LoginForm/LoginForm.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/gql/publication.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Publications/Publications.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/Home/Home.js",["217"],"import React from 'react'\nimport useAuth from '../../hooks/useAuth'\nimport './Home.scss'\n\n\nexport default function Home() {\n    const auth = useAuth()\n    return (\n        <div>\n            <h1>Estamos en la Home</h1>\n        </div>\n    )\n}\n","/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/Error404/Error404.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Auth/RegisterForm/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Header/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/gql/user.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/hooks/useAuth.js",["218"],"import { useContext } from 'react'\nimport AuthContext from '../context/AuthContext'\n\nexport default () => useContext(AuthContext)\n","/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Profile/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Publications/PreviewPublication.js/PreviewPublication.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Auth/RegisterForm/RegisterForm.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Header/Header.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Profile/Profile.js",["219"],"import React, { useState } from 'react'\nimport { Grid, Image } from 'semantic-ui-react'\nimport { useQuery } from '@apollo/client'\nimport { GET_USER } from '../../../gql/user'\nimport ImageNotFound from '../../../assets/png/avatar.png'\nimport './Profile.scss' \nimport UserNotFound from '../../UserNotFound'\nimport ModalBasic from '../../Modal/ModalBasic'\nimport AvatarForm from '../AvatarForm/AvatarForm'\nimport userAuth from '../../../hooks/useAuth'\nimport HeaderProfile from './HeaderProfile/HeaderProfile'\nimport SettingsForm from '../SettingsForm/SettingsForm'\nimport Followers from './Followers/Followers'\n\nexport default function Profile({ username, totalPublications }) {\n    const [showModal, setShowModal] = useState(false)\n    const [titleModal, setTitleModal] = useState(\"\")\n    const [childrenModal, setChildrenModal] = useState(null)\n    const { data, loading, error, refetch } = useQuery(GET_USER, { \n        variables: { username }\n    });\n    if (loading) return null\n    if(error) return <UserNotFound/>\n    const { auth } = userAuth() \n    const { name, siteWeb, description, avatar } = data.getUser\n\n    const handlerModal = (type) => {\n\n        switch (type) {\n            case \"avatar\":\n                setTitleModal(\"Cambiar Foto de Perfil\")\n                setShowModal(true)  \n                setChildrenModal(<AvatarForm setShowModal={setShowModal} auth={auth}/>)\n                break\n            case \"settings\":\n                setTitleModal(\"\")\n                setChildrenModal(\n                <SettingsForm \n                setShowModal={setShowModal}\n                setTitleModal={setTitleModal}\n                setChildrenModal={setChildrenModal}\n                getUser={data.getUser}\n                refetch={refetch}\n                auth={auth}\n                />)\n                setShowModal(true)\n                break\n            default:\n                break\n        }\n\n    }\n    return (\n        <>\n            <Grid className=\"profile\">\n                <Grid.Column width={5} className=\"profile__left\">\n                    <Image src={avatar ? avatar : ImageNotFound} avatar onClick={() => username === auth.username && handlerModal(\"avatar\")} />\n                </Grid.Column>\n                <Grid.Column width={11} className=\"profile__right\">\n                    <HeaderProfile username={username} auth={auth} handlerModal={handlerModal}/>\n                    <Followers username = {username} totalPublications={totalPublications}/>\n                    <div className=\"other\"> \n                        <p className=\"name\">{name}</p>\n                        {siteWeb&& (\n                            <a href={siteWeb} className=\"siteweb\" target=\"_blank\">\n                                {siteWeb}\n                            </a>\n                        )}\n                        {\n                            description && (\n                                <p className=\"description\">{description}</p>\n                            )\n                        }\n                    </div>\n                </Grid.Column>\n            </Grid>\n            <ModalBasic show={showModal} setShow={setShowModal} title={titleModal}>\n                {childrenModal}\n            </ModalBasic>\n        </>\n    )\n}\n","/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Header/Search/Search.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Header/RightHeader/RightHeader.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Profile/HeaderProfile/HeaderProfile.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/SettingsForm/SettingsForm.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/AvatarForm/AvatarForm.js",["220"],"import React, { useCallback, useState }from 'react'\nimport { Button } from \"semantic-ui-react\"\nimport { useDropzone } from \"react-dropzone\"\nimport { toast } from \"react-toastify\"\nimport { useMutation } from \"@apollo/client\"\nimport { UPDATE_AVATAR, GET_USER, DELETE_AVATAR } from \"../../../gql/user\"\nimport './AvatarForm.scss'\n\n\nexport default function AvatarForm({ setShowModal, auth }) {\n\n    const [loading, setLoading] = useState(false)\n\n    const [updateAvatar] = useMutation(UPDATE_AVATAR, {\n        update(cache, { data:{ updateAvatar} }){\n           const { getUser } = cache.readQuery({\n               query: GET_USER,\n               variables: { username: auth.username}\n           })\n\n           cache.writeQuery({\n                query: GET_USER,\n                variables: { username: auth.username},\n                data: {\n                    getUser: { \n                        ...getUser,\n                        avatar: updateAvatar.urlAvatar\n                    }\n                }\n           })\n        }\n    })\n\n    const [deleteAvatar] = useMutation(DELETE_AVATAR,{\n        update(cache){\n            const { getUser } = cache.readQuery({\n                query: GET_USER,\n                variables: {username: auth.username}\n            })\n\n            cache.writeQuery({\n                query: GET_USER,\n                variables: { username: auth.username },\n                data: { \n                    getUser: { \n                        ...getUser,\n                        avatar: \"\"\n                    }\n                }\n            })\n        }\n    })\n\n    const onDrop = useCallback( async (acceptedFile) => {\n        const file = acceptedFile[0];\n\n        try {\n            setLoading(true)\n            const { data } = await updateAvatar({ variables: { file } })\n            if (!data.updateAvatar.status) {\n                toast.warning(\"Error al actualizar el avatar\")\n                setLoading(false);\n            }else {\n                setLoading(false)\n                setShowModal(false)\n            }\n        } catch (error) {\n            console.log(error)\n        }\n    }, [])\n\n    const { getRootProps, getInputProps} = useDropzone({\n        accept: \"image/jpeg, image/png\",\n        noKeyboard: true,\n        multiple: false,\n        onDrop,\n    })\n\n    const onDeleteAvatar = async () => {\n        try {\n            const { data } = await deleteAvatar();\n            \n            if (!data.deleteAvatar) {\n                toast.warning(\"Error al borrar el avatar\")\n            }else {\n                setShowModal(false);\n            }\n\n        } catch (error) {\n            console.log(error)\n        }\n\n    }\n\n    return (\n        <div className=\"avatar-form\">\n            <Button  {...getRootProps()} loading={loading}>Cargar una Foto</Button>\n            <Button onClick={onDeleteAvatar}>Eliminar Foto Actual</Button>\n            <Button onClick={() =>setShowModal(false)}>Cancelar</Button>\n            <input {...getInputProps()}/>\n        </div>\n    )\n}\n","/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Profile/Followers/Followers.js",["221","222"],"import React, { useEffect, useState, useRef } from 'react'\nimport { size } from 'lodash'\nimport { useQuery, useSubscription,  useApolloClient } from '@apollo/client'\nimport { GET_FOLLOWERS, WS_GET_FOLLOWERS, GET_FOLLOWS } from '../../../../gql/follow'\nimport ModalBasic from '../../../Modal/ModalBasic'\nimport \"./Followers.scss\"\nimport UsersList from '../../UsersList/UsersList'\n\nexport default function Followers({ username, totalPublications }) {\n\n    const [followers, setFollowers] = useState(null)\n    const [showModal, setShowModal] = useState(false)\n    const [tittleModal, setTittleModal] = useState(\"\")\n    const [childrenModal, setChildrenModal] = useState(null)\n\n\n    const client = useApolloClient()\n    const isMounted = useRef(false)\n    const { \n        data: dataFollowers,\n        loading: loadingFollowers\n    } = useQuery(GET_FOLLOWERS, {\n        variables: { username }\n    })\n\n    const {\n        data: dataFollows,\n        loading: loadingFollows,\n        refetch\n    } = useQuery(GET_FOLLOWS, {\n        variables: {\n            username\n        }\n    })\n\n    const { data } = useSubscription(WS_GET_FOLLOWERS,{\n        variables: { username }\n    })\n  \n    useEffect(() => {\n        if (!isMounted.current) {\n            isMounted.current = true\n            setFollowers(dataFollowers?.getFollowers?.followers)\n            refetch()\n\n        }\n        return () => {\n            isMounted.current = false\n        }\n    }, [dataFollowers])\n\n    useEffect(() =>{\n        let followers = data?.newFollower\n        if (followers) {\n            setFollowers(followers?.followers)\n            client.writeQuery({\n                query: GET_FOLLOWERS,\n                data: { \n                    getFollowers:{ \n                        ...followers\n                    }\n                },\n                variables: {\n                    username\n                }\n            })\n        }\n    }, [data])\n\n    const openFollowers = () => {\n        setTittleModal(\"Seguidores\")\n        setShowModal(true)\n        setChildrenModal(\n            <UsersList users={followers} setShowModal={setShowModal}/>\n        )\n    }\n\n    const openFollows = () => {\n        setTittleModal(\"Seguidos\")\n        setShowModal(true)\n        setChildrenModal(\n            <UsersList users={dataFollows.getFollows} setShowModal={setShowModal}/>\n        )\n    }\n\n    if (loadingFollowers) return null\n    if(loadingFollows) return null\n    return (\n        <>\n        <div className=\"followers\">\n            <p> <span>{totalPublications}</span> Publicaciones</p>\n            <p className=\"link\" onClick={openFollowers}><span>{size(followers)}</span> Seguidores</p>\n            <p className=\"link\" onClick={openFollows}><span>{size(dataFollows.getFollows)}</span> Seguidos</p>\n        </div>\n        <ModalBasic show={ showModal } setShow={ setShowModal } title={ tittleModal } children={ childrenModal }/>\n        </>\n    )\n} \n","/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/UserNotFound/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Modal/ModalUpload/ModalUpload.js",["223"],"import React, { useCallback, useState } from 'react'\nimport { Modal, Icon, Button, Dimmer, Loader } from 'semantic-ui-react'\nimport { toast } from 'react-toastify'\nimport { useDropzone } from 'react-dropzone'\nimport { useMutation } from '@apollo/client'\nimport { PUBLISH } from '../../../gql/publication'\n\nimport './ModalUpload.scss'\n\nexport default function ModalUpload({ showModal, setShowModal }) {\n\n    const [fileUpload, setFileUpload] = useState(null)\n    const [isLoading, setIsLoading] = useState(false)\n    const [publish] = useMutation(PUBLISH)\n\n    const onDrop = useCallback((acceptedFile) => {\n        setFileUpload({\n            type: 'image',\n            file: acceptedFile[0],\n            preview: URL.createObjectURL(acceptedFile[0])\n        })\n    })\n\n    const  { getRootProps, getInputProps } = useDropzone({\n        accept:\"image/jpeg, image/png\",\n        noKeyboard: true,\n        multiple: false,\n        onDrop\n    })\n\n    const onCloseModal = () => {\n        setIsLoading(false)\n        setFileUpload(null)\n        setShowModal(false)\n    }\n\n    const onPublish = async() => {\n        try {\n            setIsLoading(true)\n            const { data } = await publish({\n                variables: {\n                    file: fileUpload.file\n                }\n            })\n            if (!data.publish.status) {\n                toast.warn(\"Error en la publicación\")\n                setIsLoading(false)\n            }else{\n                onCloseModal()\n            }\n            \n        } catch (error) {\n            console.log(error)\n        }\n    }\n    \n    return (\n        <Modal size=\"small\" open={showModal} onClose={onCloseModal} classN