/src/routes/Navigation.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/Auth/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/utils/constants.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/routes/routes.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/Auth/Auth.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/layouts/LayoutBasic.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/User.js",["194"],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/Home/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/Error404/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Auth/LoginForm/LoginForm.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/gql/publication.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Publications/Publications.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/Home/Home.js",["195"],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/pages/Error404/Error404.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Auth/RegisterForm/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Header/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/gql/user.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/hooks/useAuth.js",["196"],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Profile/index.js",[],["197","198"],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Publications/PreviewPublication.js/PreviewPublication.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Auth/RegisterForm/RegisterForm.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Header/Header.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Profile/Profile.js",["199"],"import React, { useState } from 'react'\nimport { Grid, Image } from 'semantic-ui-react'\nimport { useQuery } from '@apollo/client'\nimport { GET_USER } from '../../../gql/user'\nimport ImageNotFound from '../../../assets/png/avatar.png'\nimport './Profile.scss' \nimport UserNotFound from '../../UserNotFound'\nimport ModalBasic from '../../Modal/ModalBasic'\nimport AvatarForm from '../AvatarForm/AvatarForm'\nimport userAuth from '../../../hooks/useAuth'\nimport HeaderProfile from './HeaderProfile/HeaderProfile'\nimport SettingsForm from '../SettingsForm/SettingsForm'\nimport Followers from './Followers/Followers'\n\nexport default function Profile({ username, totalPublications }) {\n    const [showModal, setShowModal] = useState(false)\n    const [titleModal, setTitleModal] = useState(\"\")\n    const [childrenModal, setChildrenModal] = useState(null)\n    const { data, loading, error, refetch } = useQuery(GET_USER, { \n        variables: { username }\n    });\n    if (loading) return null\n    if(error) return <UserNotFound/>\n    const { auth } = userAuth() \n    const { name, siteWeb, description, avatar } = data.getUser\n\n    const handlerModal = (type) => {\n\n        switch (type) {\n            case \"avatar\":\n                setTitleModal(\"Cambiar Foto de Perfil\")\n                setShowModal(true)  \n                setChildrenModal(<AvatarForm setShowModal={setShowModal} auth={auth}/>)\n                break\n            case \"settings\":\n                setTitleModal(\"\")\n                setChildrenModal(\n                <SettingsForm \n                setShowModal={setShowModal}\n                setTitleModal={setTitleModal}\n                setChildrenModal={setChildrenModal}\n                getUser={data.getUser}\n                refetch={refetch}\n                auth={auth}\n                />)\n                setShowModal(true)\n                break\n            default:\n                break\n        }\n\n    }\n    return (\n        <>\n            <Grid className=\"profile\">\n                <Grid.Column width={5} className=\"profile__left\">\n                    <Image src={avatar ? avatar : ImageNotFound} avatar onClick={() => username === auth.username && handlerModal(\"avatar\")} />\n                </Grid.Column>\n                <Grid.Column width={11} className=\"profile__right\">\n                    <HeaderProfile username={username} auth={auth} handlerModal={handlerModal}/>\n                    <Followers username = {username} totalPublications={totalPublications}/>\n                    <div className=\"other\"> \n                        <p className=\"name\">{name}</p>\n                        {siteWeb&& (\n                            <a href={siteWeb} className=\"siteweb\" target=\"_blank\">\n                                {siteWeb}\n                            </a>\n                        )}\n                        {\n                            description && (\n                                <p className=\"description\">{description}</p>\n                            )\n                        }\n                    </div>\n                </Grid.Column>\n            </Grid>\n            <ModalBasic show={showModal} setShow={setShowModal} title={titleModal}>\n                {childrenModal}\n            </ModalBasic>\n        </>\n    )\n}\n","/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Header/Search/Search.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Header/RightHeader/RightHeader.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Profile/HeaderProfile/HeaderProfile.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/SettingsForm/SettingsForm.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/AvatarForm/AvatarForm.js",["200"],"import React, { useCallback, useState }from 'react'\nimport { Button } from \"semantic-ui-react\"\nimport { useDropzone } from \"react-dropzone\"\nimport { toast } from \"react-toastify\"\nimport { useMutation } from \"@apollo/client\"\nimport { UPDATE_AVATAR, GET_USER, DELETE_AVATAR } from \"../../../gql/user\"\nimport './AvatarForm.scss'\n\n\nexport default function AvatarForm({ setShowModal, auth }) {\n\n    const [loading, setLoading] = useState(false)\n\n    const [updateAvatar] = useMutation(UPDATE_AVATAR, {\n        update(cache, { data:{ updateAvatar} }){\n           const { getUser } = cache.readQuery({\n               query: GET_USER,\n               variables: { username: auth.username}\n           })\n\n           cache.writeQuery({\n                query: GET_USER,\n                variables: { username: auth.username},\n                data: {\n                    getUser: { \n                        ...getUser,\n                        avatar: updateAvatar.urlAvatar\n                    }\n                }\n           })\n        }\n    })\n\n    const [deleteAvatar] = useMutation(DELETE_AVATAR,{\n        update(cache){\n            const { getUser } = cache.readQuery({\n                query: GET_USER,\n                variables: {username: auth.username}\n            })\n\n            cache.writeQuery({\n                query: GET_USER,\n                variables: { username: auth.username },\n                data: { \n                    getUser: { \n                        ...getUser,\n                        avatar: \"\"\n                    }\n                }\n            })\n        }\n    })\n\n    const onDrop = useCallback( async (acceptedFile) => {\n        const file = acceptedFile[0];\n\n        try {\n            setLoading(true)\n            const { data } = await updateAvatar({ variables: { file } })\n            if (!data.updateAvatar.status) {\n                toast.warning(\"Error al actualizar el avatar\")\n                setLoading(false);\n            }else {\n                setLoading(false)\n                setShowModal(false)\n            }\n        } catch (error) {\n            console.log(error)\n        }\n    }, [])\n\n    const { getRootProps, getInputProps} = useDropzone({\n        accept: \"image/jpeg, image/png\",\n        noKeyboard: true,\n        multiple: false,\n        onDrop,\n    })\n\n    const onDeleteAvatar = async () => {\n        try {\n            const { data } = await deleteAvatar();\n            \n            if (!data.deleteAvatar) {\n                toast.warning(\"Error al borrar el avatar\")\n            }else {\n                setShowModal(false);\n            }\n\n        } catch (error) {\n            console.log(error)\n        }\n\n    }\n\n    return (\n        <div className=\"avatar-form\">\n            <Button  {...getRootProps()} loading={loading}>Cargar una Foto</Button>\n            <Button onClick={onDeleteAvatar}>Eliminar Foto Actual</Button>\n            <Button onClick={() =>setShowModal(false)}>Cancelar</Button>\n            <input {...getInputProps()}/>\n        </div>\n    )\n}\n","/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/Profile/Followers/Followers.js",["201","202"],"import React, { useEffect, useState, useRef } from 'react'\nimport { size } from 'lodash'\nimport { useQuery, useSubscription,  useApolloClient } from '@apollo/client'\nimport { GET_FOLLOWERS, WS_GET_FOLLOWERS, GET_FOLLOWS } from '../../../../gql/follow'\nimport ModalBasic from '../../../Modal/ModalBasic'\nimport \"./Followers.scss\"\nimport UsersList from '../../UsersList/UsersList'\n\nexport default function Followers({ username, totalPublications }) {\n\n    const [followers, setFollowers] = useState(null)\n    const [showModal, setShowModal] = useState(false)\n    const [tittleModal, setTittleModal] = useState(\"\")\n    const [childrenModal, setChildrenModal] = useState(null)\n\n\n    const client = useApolloClient()\n    const isMounted = useRef(false)\n    const { \n        data: dataFollowers,\n        loading: loadingFollowers\n    } = useQuery(GET_FOLLOWERS, {\n        variables: { username }\n    })\n\n    const {\n        data: dataFollows,\n        loading: loadingFollows,\n        refetch\n    } = useQuery(GET_FOLLOWS, {\n        variables: {\n            username\n        }\n    })\n\n    const { data } = useSubscription(WS_GET_FOLLOWERS,{\n        variables: { username }\n    })\n  \n    useEffect(() => {\n        if (!isMounted.current) {\n            isMounted.current = true\n            setFollowers(dataFollowers?.getFollowers?.followers)\n            refetch()\n\n        }\n        return () => {\n            isMounted.current = false\n        }\n    }, [dataFollowers])\n\n    useEffect(() =>{\n        let followers = data?.newFollower\n        if (followers) {\n            setFollowers(followers?.followers)\n            client.writeQuery({\n                query: GET_FOLLOWERS,\n                data: { \n                    getFollowers:{ \n                        ...followers\n                    }\n                },\n                variables: {\n                    username\n                }\n            })\n        }\n    }, [data])\n\n    const openFollowers = () => {\n        setTittleModal(\"Seguidores\")\n        setShowModal(true)\n        setChildrenModal(\n            <UsersList users={followers} setShowModal={setShowModal}/>\n        )\n    }\n\n    const openFollows = () => {\n        setTittleModal(\"Seguidos\")\n        setShowModal(true)\n        setChildrenModal(\n            <UsersList users={dataFollows.getFollows} setShowModal={setShowModal}/>\n        )\n    }\n\n    if (loadingFollowers) return null\n    if(loadingFollows) return null\n    return (\n        <>\n        <div className=\"followers\">\n            <p> <span>{totalPublications}</span> Publicaciones</p>\n            <p className=\"link\" onClick={openFollowers}><span>{size(followers)}</span> Seguidores</p>\n            <p className=\"link\" onClick={openFollows}><span>{size(dataFollows.getFollows)}</span> Seguidos</p>\n        </div>\n        <ModalBasic show={ showModal } setShow={ setShowModal } title={ tittleModal } children={ childrenModal }/>\n        </>\n    )\n} \n","/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/UserNotFound/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Modal/ModalUpload/ModalUpload.js",["203"],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Modal/ModalBasic/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/gql/follow.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/UsersList/UsersList.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/WebPageForm/WebPageForm.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/DescriptionForm/DescriptionForm.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/Modal/ModalBasic/ModalBasic.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/UserNotFound/UserNotFound.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/EmailForm/EmailForm.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/PasswordForm/index.js",[],"/home/dreadfulcrazy/Documentos/Instaclone/client/src/components/User/PasswordForm/PasswordForm.js",[],{"ruleId":"204","severity":1,"message":"205","line":44,"column":6,"nodeType":"206","endLine":44,"endColumn":26,"suggestions":"207"},{"ruleId":"208","severity":1,"message":"209","line":7,"column":11,"nodeType":"210","messageId":"211","endLine":7,"endColumn":15},{"ruleId":"212","severity":1,"message":"213","line":4,"column":1,"nodeType":"214","endLine":4,"endColumn":45},{"ruleId":"215","replacedBy":"216"},{"ruleId":"217","replacedBy":"218"},{"ruleId":"219","severity":1,"message":"220","line":65,"column":29,"nodeType":"221","messageId":"222","endLine":65,"endColumn":83,"fix":"223"},{"ruleId":"204","severity":1,"message":"224","line":70,"column":8,"nodeType":"206","endLine":70,"endColumn":10,"suggestions":"225"},{"rul